						"hasFluff": {
							"const": true
						},
						"hasFluffImages": {
							"const": true
						},
						"_copy": {
							"$ref": "../util.json#/$defs/copyBlock_copy_generic"
						}
					},
					"additionalProperties": false,
					"required": [
						"_copy"
					]
				}
			]
