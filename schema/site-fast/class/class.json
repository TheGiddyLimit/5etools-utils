						"preparedSpells": {
							"$ref": "#/$defs/preparedSpells"
						},
						"preparedSpellsProgression": {
							"$ref": "#/$defs/preparedSpellsProgression"
						},
						"spellsKnownProgression": {
							"$ref": "#/$defs/spellsKnownProgression"
						},
						"spellsKnownProgressionFixed": {
